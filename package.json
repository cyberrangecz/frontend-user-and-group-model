{
    "name": "user-and-group-model",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint && npm run prettier-check",
        "e2e": "ng e2e",
        "ci-test": "ng test --browsers ChromeHeadless --watch=false",
        "prettier-check": "prettier --check \"projects/**/*.ts\"",
        "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
        "build-library": "ng build kypo-user-and-group-model --configuration production",
        "create-package": "cd dist/kypo-user-and-group-model && npm pack",
        "build-and-pack": "npm install && npm run build-library && npm run create-package",
        "ci-update-version": "cd ./projects/kypo-user-and-group-model/ && npm version $TAG_VERSION",
        "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
        "ci-publish-package": "cd dist/kypo-user-and-group-model && npm publish"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    },
    "dependencies": {
        "@angular/animations": "^18.2.13",
        "@angular/common": "^18.2.13",
        "@angular/compiler": "^18.2.13",
        "@angular/core": "^18.2.13",
        "@angular/forms": "^18.2.13",
        "@angular/platform-browser": "^18.2.13",
        "@angular/platform-browser-dynamic": "^18.2.13",
        "@angular/router": "^18.2.13",
        "rxjs": "^7.8.1",
        "tslib": "^2.7.0",
        "zone.js": "^0.14.10"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.2.14",
        "@angular-eslint/builder": "^18.4.3",
        "@angular-eslint/eslint-plugin": "^18.4.3",
        "@angular-eslint/eslint-plugin-template": "^18.4.3",
        "@angular-eslint/schematics": "^18.4.3",
        "@angular-eslint/template-parser": "^18.4.3",
        "@angular/cli": "^18.2.14",
        "@angular/compiler-cli": "^18.2.13",
        "@angular/language-service": "^18.2.13",
        "@types/jasmine": "^5.1.4",
        "@types/jasminewd2": "^2.0.13",
        "@types/node": "^22.6.1",
        "@typescript-eslint/eslint-plugin": "^8.23.0",
        "@typescript-eslint/parser": "^8.23.0",
        "eslint": "^9.19.0",
        "husky": "^9.1.7",
        "jasmine-core": "^5.3.0",
        "jasmine-spec-reporter": "^7.0.0",
        "karma": "^6.4.4",
        "karma-chrome-launcher": "^3.2.0",
        "karma-coverage-istanbul-reporter": "^3.0.3",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "ng-packagr": "^18.2.1",
        "prettier": "^3.4.2",
        "protractor": "~7.0.0",
        "ts-node": "^10.9.2",
        "typescript": "5.5.4"
    }
}