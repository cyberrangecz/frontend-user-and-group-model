{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1, 
  "newProjectRoot": "projects",
  "projects": {
    "kypo-user-and-group-model": {
      "projectType": "library",
      "root": "projects/kypo-user-and-group-model",
      "sourceRoot": "projects/kypo-user-and-group-model/src",
      "prefix": "kypo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/kypo-user-and-group-model/tsconfig.lib.json",
            "project": "projects/kypo-user-and-group-model/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/kypo-user-and-group-model/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kypo-user-and-group-model/src/test.ts",
            "tsConfig": "projects/kypo-user-and-group-model/tsconfig.spec.json",
            "karmaConfig": "projects/kypo-user-and-group-model/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/kypo-user-and-group-model/tsconfig.lib.json",
              "projects/kypo-user-and-group-model/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "kypo-user-and-group-model"
}
